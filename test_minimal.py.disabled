from flask import Flask

app = Flask(__name__)

@app.route('/')
def home():
    return "<h1>Home Works</h1><a href='/batch'>Go to Batch</a>"

@app.route('/batch')
def batch():
    return "<h1>Batch Works!</h1>"

if __name__ == '__main__':
    print("Starting minimal test app...")
    app.run(debug=True, port=5001)